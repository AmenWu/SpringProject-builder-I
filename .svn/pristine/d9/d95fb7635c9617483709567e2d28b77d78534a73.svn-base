<script type="text/javascript">
	function freshFrame(){
		$(".J_iframe").each(function(){
			var frame = $(this);
			if(frame.css("display") != 'none'){
				var name = frame.attr("name");
				$(window.frames[name].document).find("#pageForm").submit();
			}
		})
	}
	function freshPremission(){
		$(".J_iframe").each(function(){
			var frame = $(this);
			if(frame.css("display") != 'none'){
				var name = frame.attr("name");
				$(window.frames[name].document).find("#reloadPermison").trigger("click");
			}
		})
	}
</script>
