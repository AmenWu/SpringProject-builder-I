	<script>
		if (window.top != window.self) {
			window.top.location = window.location;
		}
		//左侧菜单样式列表 详情参考 icon.html 根据项目需求调整
		var iconArr = new Array("fa fa-list-ul","fa fa-bar-chart-o","fa fa-edit","fa  fa-desktop","fa fa-table","fa fa-picture-o","fa fa-columns","fa fa-film");
		$(function(){
			$(".tempClass").each(function(i){
				var classtar= iconArr[i];
				if(typeof(classtar) != "undefined"){
					$(this).attr("class",classtar);
				}
			})
		})
	</script>
